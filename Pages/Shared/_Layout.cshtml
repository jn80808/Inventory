<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="author" content="Joy Ng" >
    <title>@ViewData["Title"] - Inventory</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Inventory1.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/site.css"><!-- custom CSS file -->
    <link rel="stylesheet" href="~/css/styles.css">
    <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" rel="stylesheet" />
    <link rel="icon" href="https://i.pinimg.com/736x/da/bc/fc/dabcfc73d086aeae06d1d55df18e2384.jpg" type="image/x-icon">
     <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">
    @* <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.8/dist/umd/popper.min.js" integrity="sha384-I7E8VVD/ismYTF4hNIPjVp/Zjvgyol6VFvRkX/vR+Vc4jQkC+hVqc2pM8ODewa9r" crossorigin="anonymous"></script> *@
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.min.js" integrity="sha384-0pUGZvbkm6XF6gxjEnlmuGrJXVbNuzT9qBBavbLwCsOGabYfZo0T0to5eqruptLy" crossorigin="anonymous"></script>

</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container">
                <a class="navbar-brand" asp-area="" asp-page="/Index">Inventory</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Privacy">Privacy</a>
                        </li>
                                                <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Inventory_Products/Index">ProductInventory</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>



        @* Footer *@
        <footer class="py-5 bg-dark text-light">
            <div class="container">
                <div class="row">
                    <div class="col-6 col-md-3 mb-4">
                        <h5 class="text-uppercase font-weight-bold">Company</h5>
                        <ul class="nav flex-column">
                            <li class="nav-item mb-2"><a href="#" class="nav-link p-0 text-light hover-link">Home</a></li>
                            <li class="nav-item mb-2"><a href="#" class="nav-link p-0 text-light hover-link">Features</a></li>
                            <li class="nav-item mb-2"><a href="#" class="nav-link p-0 text-light hover-link">Pricing</a></li>
                            <li class="nav-item mb-2"><a href="#" class="nav-link p-0 text-light hover-link">FAQs</a></li>
                            <li class="nav-item mb-2"><a href="#" class="nav-link p-0 text-light hover-link">About</a></li>
                        </ul>
                    </div>

                    <div class="col-6 col-md-3 mb-4">
                        <h5 class="text-uppercase font-weight-bold">Resources</h5>
                        <ul class="nav flex-column">
                            <li class="nav-item mb-2"><a href="#" class="nav-link p-0 text-light hover-link">Documentation</a></li>
                            <li class="nav-item mb-2"><a href="#" class="nav-link p-0 text-light hover-link">Blog</a></li>
                            <li class="nav-item mb-2"><a href="#" class="nav-link p-0 text-light hover-link">API</a></li>
                            <li class="nav-item mb-2"><a href="#" class="nav-link p-0 text-light hover-link">Privacy Policy</a></li>
                            <li class="nav-item mb-2"><a href="#" class="nav-link p-0 text-light hover-link">Terms of Service</a></li>
                        </ul>
                    </div>

                    <div class="col-6 col-md-3 mb-4">
                        <h5 class="text-uppercase font-weight-bold">Support</h5>
                        <ul class="nav flex-column">
                            <li class="nav-item mb-2"><a href="#" class="nav-link p-0 text-light hover-link">Help Center</a></li>
                            <li class="nav-item mb-2"><a href="#" class="nav-link p-0 text-light hover-link">Contact Us</a></li>
                            <li class="nav-item mb-2"><a href="#" class="nav-link p-0 text-light hover-link">Community</a></li>
                        </ul>
                    </div>

                    <div class="col-md-3 mb-4">
                        <h5 class="text-uppercase font-weight-bold">Subscribe</h5>
                        <p class="text-light">Stay updated with the latest news and features by subscribing to our newsletter.</p>
                        <form>
                            <div class="d-flex flex-column flex-sm-row gap-2">
                                <input id="newsletter1" type="email" class="form-control border-0 rounded-0" placeholder="Enter your email">
                                <button class="btn btn-primary px-4 py-2" type="submit">Subscribe</button>
                            </div>
                        </form>
                    </div>
                </div>

                <div class="d-flex justify-content-between align-items-center py-4 border-top border-light">
                    <p class="mb-0">© Duckers Company, Inc. All rights reserved.</p>
                    <ul class="list-unstyled d-flex mb-0">
                        <li class="ms-3"><a href="#" class="link-light"><i class="bi bi-twitter"></i></a></li>
                        <li class="ms-3"><a href="#" class="link-light"><i class="bi bi-instagram"></i></a></li>
                        <li class="ms-3"><a href="#" class="link-light"><i class="bi bi-facebook"></i></a></li>
                    </ul>
                </div>
            </div>
        </footer>


    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>


    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>